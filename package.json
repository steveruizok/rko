{
  "name": "rko",
  "version": "0.6.4",
  "author": {
    "name": "Steve Ruiz",
    "url": "https://twitter.com/steveruizok"
  },
  "repository": "https://github.com/steveruizok/rko",
  "keywords": [
    "state",
    "react",
    "undo",
    "redo",
    "persistence"
  ],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "browserlist": [
    ">5%",
    "ie11",
    "chrome",
    "firefox",
    "safari",
    "not dead"
  ],
  "targets": {
    "default": {
      "distDir": "./dist",
      "isLibrary": true
    }
  },
  "sourceMap": "inline",
  "scripts": {
    "start": "parcel watch & cd example && yarn start",
    "build": "parcel build && tsc --project tsconfig.build.json",
    "check": "tsc --noEmit",
    "lint": "eslint"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/transformer-js": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "parcel": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1",
    "typescript": ">=3.0.0"
  },
  "dependencies": {
    "idb-keyval": "^6.0.3",
    "zustand": "^3.6.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": "^16.8 || ^17.0"
  }
}
